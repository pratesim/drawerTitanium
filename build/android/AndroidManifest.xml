<?xml version="1.0" ?><manifest android:installLocation="preferExternal" android:versionCode="3" android:versionName="1.0.2" package="ti.drawerdemo" xmlns:android="http://schemas.android.com/apk/res/android">
	<uses-sdk android:minSdkVersion="18" android:targetSdkVersion="18"/>

	<!-- TI_MANIFEST -->

