".container": {
	orientationModes: [Ti.UI.PORTRAIT,Ti.UI.LANDSCAPE_LEFT,Ti.UI.LANDSCAPE_RIGHT,Ti.UI.UPSIDE_PORTRAIT],
	navBarHidden: true,
	exitOnClose: false,
	layout: 'vertical',
	backgroundColor: '#cacaca',
	width: Ti.UI.FILL
},
"#scrollView":{
	top: "0dp",
	layout: "vertical",
	width: Ti.UI.FILL,
	height: Ti.UI.FILL,
	//backgroundColor:'white'
	backgroundGradient: {
        type: 'linear',
        startPoint: { x: '0%', y: '0%' },
        endPoint: { x: '0%', y: '100%' },
        colors: [ { color: '#050607', offset: 0.0}, { color: '#272D33', offset: 1.0 } ],
    }
},
".scrollContainer":{
    top: "20dp",
    left: "10dp",
    right: "10dp",
    layout: "vertical",
    backgroundColor: "transparent",
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE
},
".inputLabel":{
    font: {
        fontSize: "16dp",
        fontWeight: "bold"
    },
    textAlign: Ti.UI.TEXT_ALIGNMENT_LEFT,
    color: "#BEBEBE",
    left: "0dp",
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE
},
".separationLine":{
	backgroundColor: "#494C4F",
	height: "3dp",
	width: Ti.UI.FILL,
	top: "0dp"
},
"TextArea":{
    top: "5dp",
    left: "0dp",
    color: "white",
    textAlign: Ti.UI.TEXT_ALIGNMENT_LEFT,
    width: Ti.UI.FILL,
    /*backgroundColor: "transparent"*/
},
".buttonGroup":{
    layout: 'horizontal',
    width: Ti.UI.FILL,
    height: Ti.UI.SIZE,
    backgroundColor: "transparent"
},
"Button":{
    font: {
            fontSize: "16dp"
    },
    textAlign: Ti.UI.TEXT_ALIGNMENT_CENTER,
    height: Ti.UI.SIZE,
    width: "50%"
},
"#repoimage":{
	top: "5dp"
},
"#takephoto":{
    left: "0%",
},
"#send":{
    right: "0%"
}